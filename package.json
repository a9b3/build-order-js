    "fs-extra": "0.30.0",
    "invariant": "^2.2.1",
    "lodash": "4.16.6"
  },
  "devDependencies": {
    "eslint": "4.15.0",
    "eslint-config-esayemm": "2.0.19",
    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "prettier": "1.9.2",
    "release-it": "5.2.0"
  },
  "babel": {
    "presets": [
      "./node_modules/jbs-node/configs/babel-preset-jbs-node.js"
    ],
    "ignore": [
      "**/templates/**/*.js",
      "**/templates/*.js"
    ]
  }
