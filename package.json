{
  "name": "build-order-js",
  "version": "3.0.3",
  "description": "Set up javascript development environment procedurally using tasks",
  "author": "Sam Lau <esayemm@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/esayemm/build-order-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/esayemm/build-order-js"
  },
  "scripts": {
    "build": "rm -rf build && ./node_modules/babel-cli/bin/babel.js src --out-dir build --copy-files",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "prepare": "npm run build",
    "preversion": "npm run lint && npm run test",
    "version": "npm publish",
    "postversion": "git add . && git push && git push --tags"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "chalk": "^1.1.3",
    "commander-shepard": "^2.0.10",
    "eslint": "^3.15.0",
    "eslint-config-esayemm": "^2.0.1",
    "expect": "^1.20.2",
    "fs-extra": "^0.30.0",
    "invariant": "^2.2.1",
    "lodash": "^4.16.6",
    "mocha": "^3.2.0"
  },
  "preferGlobal": true,
  "bin": {
    "bojs-dev": "./index.js",
    "bojs": "./bin.js"
  },
  "files": [
    "build/",
    "bin.js",
    "index.js"
  ]
}
