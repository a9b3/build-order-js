    "bojs": "./bin.js"
  },
  "files": [
    "build/",
    "bin.js",
    "index.js"
  ],
  "dependencies": {
    "app-module-path": "^2.2.0",
    "chalk": "^1.1.3",
    "commander-shepard": "^3.0.3",
