{
  "name": "build-order-js",
  "version": "3.2.13",
  "description": "Set up javascript development environment procedurally using tasks",
  "author": "Sam Lau <esayemm@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/esayemm/build-order-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/esayemm/build-order-js"
  },
  "preferGlobal": true,
  "bin": {
